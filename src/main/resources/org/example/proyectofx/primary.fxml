<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<Pane xmlns="http://javafx.com/javafx/23.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="viewController.BibliotecaViewController"
      prefHeight="630.0" prefWidth="780.0">

   <children>
      <Button fx:id="btnBibliotecario" layoutX="217.0" layoutY="426.0" mnemonicParsing="false" text="Bibliotecario">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
         </padding>
      </Button>
      <Button fx:id="btnUsuario" layoutX="350.0" layoutY="426.0" mnemonicParsing="false" text="Usuario" onAction="#onOpenInicioUsuario">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
         </padding>
      </Button>
      <Button fx:id="btnAdministrador" layoutX="459.0" layoutY="426.0" mnemonicParsing="false" text="Administrador">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
         </padding>
      </Button>
      <Text layoutX="128.0" layoutY="136.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Bienvenidos a la Biblioteca UQ">
         <font>
            <Font size="39.0"/>
         </font>
      </Text>
      <Text layoutX="268.0" layoutY="352.0" strokeType="OUTSIDE" strokeWidth="0.0" text="¿Qué relación tiene con la UQ?">
         <font>
            <Font size="18.0"/>
         </font>
      </Text>
      <ImageView fitHeight="145.0" fitWidth="259.0" layoutX="261.0" layoutY="176.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image/>
         </image>
      </ImageView>
   </children>
</Pane>
